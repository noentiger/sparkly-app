{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "__DEV__": true
    },
    "extends": "airbnb",
    "rules": {
        "react/sort-comp": 0,
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "react/jsx-filename-extension": 0,
        "import/no-named-as-default-member": 0,
        "import/prefer-default-export": 0,
        "no-use-before-define": 0,
        "no-else-return": 0,
        "no-console": 0,
        "react/jsx-no-bind": 0,
        "import/extensions": 0,
        "no-plusplus": 0
    }
}
